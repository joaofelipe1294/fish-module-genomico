<%= render 'layouts/navbar' %>

<div class="p-5" style="background-color: rgba(0, 0, 0, 0.65)">

  <% @scanning.scanning_images.each do |image| %>

    <% image.scanned_cells.each do |cell| %>
      <div class="card d-inline-block" style="margin-left: 15px; margin-bottom: 15px; margin-right: 15px;">
        <%= image_tag cell.image, style: {width: 125, height: 125} %>
      </div>
    <% end %>

  <% end %>

</div>
